@model Dictionary<int, Reservation>

<table class="table table-bordered table-striped">
    <thead>
        <tr class="success">
            <th>Lesson hour</th>
            <th style="width: 85%">Reservation</th>
        </tr>
    </thead>
    <tbody>
        @foreach (int hour in Model.Keys)
        {
            if (Model[hour] != null)
            {
                <tr>
                    <td>@hour</td>
                    <td><b>@Model[hour].Subject</b> - @Model[hour].Reserver</td>
                </tr>
            }
            else
            {
                <tr>
                    <td>@hour</td>
                    <td></td>
                </tr>
            }
        }
    </tbody>
</table>

